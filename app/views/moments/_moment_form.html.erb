<div class="upload-moment">
  <%= simple_form_for @moment do |f| %>
    <div class="add-photos">
      <h6><strong>Add photos to your listing</strong></h6>
      <%= f.input :media, label: "Photos help guests imagine renting your. You can start with one and add more after you publish" %>
      <%= f.input :media_cache, as: :hidden %>
    </div>
    <%= f.submit 'Publish', class: "cute-button" %>
  <% end %>
</div>
